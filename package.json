{
  "name": "capacitor-pica-network-logger",
  "version": "0.2.0",
  "description": "Capacitor HTTP network logger with debug-only native capture",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linakis/capacitor-pica-network-logger.git"
  },
  "homepage": "https://github.com/linakis/capacitor-pica-network-logger#readme",
  "bugs": {
    "url": "https://github.com/linakis/capacitor-pica-network-logger/issues"
  },
